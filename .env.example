# OpenAI API Key for server-side usage (Supabase Edge Function)
OPENAI_API_KEY=your_server_side_api_key_here

# OpenAI API Key for client-side usage (direct React calls)
# This is used for the specialized listeners like task detection
# WARNING: Client-side API keys are exposed to the browser
# Use with caution and consider server-side proxies in production

# For Vite, environment variables must start with VITE_
VITE_OPENAI_CLIENT_KEY=your_client_side_api_key_here

# For Create React App or other environments
REACT_APP_OPENAI_CLIENT_KEY=your_client_side_api_key_here  